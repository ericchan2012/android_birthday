<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="more_row_wrapper">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/common_white_shadow_bg</item>
        <item name="android:paddingLeft">0.0dip</item>
        <item name="android:paddingRight">0.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_marginTop">8.0dip</item>
        <item name="android:layout_marginRight">10.0dip</item>
        <item name="android:layout_marginBottom">8.0dip</item>
    </style>
    <style name="text_big_default">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@color/list_item_title_text_color</item>
    </style>
    <style name="text_small_default_black">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">@color/list_item_title_text_color</item>
    </style>

    <style name="default_body_container">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/border_bg</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="more_row_layout">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/common_menu_click_background</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">42.0dip</item>
        <item name="android:layout_marginLeft">2.0dip</item>
        <item name="android:layout_marginTop">2.0dip</item>
        <item name="android:layout_marginRight">2.0dip</item>
        <item name="android:layout_marginBottom">2.0dip</item>
    </style>

    <style name="more_row_title_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/black_text_color</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_weight">0.8</item>
    </style>

    <style name="more_right_arrow">
        <item name="android:layout_gravity">center</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/right_arr</item>
        <item name="android:layout_weight">0.2</item>
    </style>
    <style name="more_right_toggle">
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/btn_toggle_bg</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.2</item>
        <item name="android:textOff">@null</item>
        <item name="android:textOn">@null</item>
        <item name="android:disabledAlpha">?android:disabledAlpha</item>
    </style>

    <style name="default_linearlayout_background">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/border_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="headerNav">
        <item name="android:background">@drawable/header_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">43.0dip</item>
        <item name="android:baselineAligned">true</item>
    </style>

    <style name="headerNavText">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/app_name</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="head_nav_left_button">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/back_btn</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">2.0dip</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="head_nav_right_button">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/nav_button_right</item>
        <item name="android:paddingLeft">2.0dip</item>
        <item name="android:paddingRight">2.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">5.0dip</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="login_reg_new_yellow_button">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/whiteTextColor</item>
        <item name="android:background">@drawable/yellow_btn</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="login_reg_new_green_button">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/whiteTextColor</item>
        <item name="android:background">@drawable/green_btn</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="login_reg_row_container">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">15.0dip</item>
    </style>

    <style name="login_reg_row_wrapper">
        <item name="android:orientation">vertical</item>
        <item name="android:background">@drawable/common_white_shadow_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="login_reg_row_layout">
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">10.0dip</item>
        <item name="android:layout_marginBottom">10.0dip</item>
    </style>

    <style name="login_reg_row_title_text">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ff5bbd2c</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.2</item>
    </style>

    <style name="divider_line">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:src">@color/gray_new_color</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="login_reg_row_step">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/whiteTextColor</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/shape_green_circle</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.1</item>
    </style>

    <style name="login_reg_row_edit_text">
        <item name="android:textSize">16.0sp</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.7</item>
    </style>

    <style name="MyDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="StyleBarTitle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:shadowColor">@color/black</item>
        <item name="android:shadowDx">1</item>
        <item name="android:shadowDy">1</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="ptr_headerContainer">
        <item name="android:gravity">bottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0px</item>
    </style>

    <style name="ptr_header">
        <item name="android:padding">5.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="ptr_spinner" parent="@android:style/Widget.ProgressBar.Small.Inverse">
        <item name="android:paddingLeft">25.0dip</item>
        <item name="android:paddingRight">25.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:indeterminate">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="ptr_text">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="coupon_theme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:listViewStyle">@style/list_view</item>
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="top_left_button" parent="@android:style/Widget.Button">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/delete</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:visibility">invisible</item>
        <item name="android:layout_marginLeft">8.0dip</item>
    </style>

    <style name="radiogroup">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginTop">6dip</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="top_right_button" parent="@android:style/Widget.Button">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/nav_button_right</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:visibility">invisible</item>
    </style>

    <style name="top_left_radio" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:gravity">center</item>
