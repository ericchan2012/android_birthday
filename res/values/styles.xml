    <style name="top_right_radio" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/tab_right_bg</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="bottom_radio" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:ellipsize">marquee</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/bottom_radio_button_bg</item>
        <item name="android:paddingTop">@dimen/bottom_tab_padding_up</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:singleLine">true</item>
